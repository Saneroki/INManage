<AnchorPane prefHeight="329.0" prefWidth="493.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.gui.windows.AddProject">
   <children>
      <AnchorPane prefHeight="333.0" prefWidth="491.0">
