               <font>
                  <Font name="Avenir Next Condensed Regular" size="17.0" />
               </font>
            </Label>
            <TextField id="addprojname" fx:id="addProjNameField" layoutX="14.0" layoutY="55.0" prefHeight="27.0" prefWidth="314.0" promptText="Project name" />
            <TextArea id="addprojdesc" fx:id="addProjDescField" layoutX="14.0" layoutY="134.0" prefHeight="137.0" prefWidth="454.0" promptText="Project description" wrapText="true" />
