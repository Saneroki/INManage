<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.gui.layouts.AddProject">
   <AnchorPane minHeight="400.0" minWidth="600.0" prefHeight="768.0" prefWidth="1024.0" styleClass="root" stylesheets="@../Style.css">
      <Label layoutX="14.0" layoutY="31.0" styleClass="text" stylesheets="@../Style.css" text="Name">
         <font>
            <Font name="Avenir Next Condensed Regular" size="17.0" />
         </font>
      </Label>
      <TextField id="addprojname" fx:id="addProjNameField" layoutX="14.0" layoutY="55.0" prefHeight="27.0" prefWidth="314.0" promptText="Project name" />
      <TextArea id="addprojdesc" fx:id="addProjDescField" layoutX="14.0" layoutY="134.0" prefHeight="137.0" prefWidth="454.0" promptText="Project description" wrapText="true" />
      <Label layoutX="16.0" layoutY="110.0" styleClass="text" text="Description">
         <font>
            <Font name="Avenir Next Condensed Regular" size="17.0" />
         </font>
      </Label>
      <Button id="addprojconfirmbtn" fx:id="addProjConfirmBtn" layoutX="17.0" layoutY="284.0" mnemonicParsing="false" onAction="#addProjectConfirmAction" stylesheets="@../Style.css" text="Confirm">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Button>
      <Button id="addprojcancelbtn" fx:id="addProjCancelBtn" layoutX="110.0" layoutY="284.0" mnemonicParsing="false" onAction="#addProjectCancelButton" stylesheets="@../Style.css" text="Cancel">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Button>
   </AnchorPane>
</AnchorPane>
